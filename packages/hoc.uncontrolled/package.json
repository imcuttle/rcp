{
  "name": "@rcp/hoc.uncontrolled",
  "author": "imcuttle",
  "description": "The high order component for creating uncontrolled component",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "test": "npx jest",
    "dist": "run-p --print-label \"dist:**\"",
    "dist:es5": "tsc $TSC_OPTIONS --target es5",
    "dist:es6": "tsc $TSC_OPTIONS --target es6 --outDir es6",
    "dev": "TSC_OPTIONS=-w npm run dist",
    "prepublishOnly": "npm run dist && npm test",
    "version": "npm run doc",
    "doc": "documentation --markdown-toc=false readme index.js -a public -s \"API\" && git add README.md"
  },
  "keywords": [
    "imcuttle",
    "hoc",
    "uncontrolled",
    "react"
  ],
  "engines": {
    "node": ">=6"
  },
  "main": "index.js",
  "typings": "index.d.ts",
  "version": "2.1.8",
  "dependencies": {
    "@rcp/util.createlogger": "^1.1.4",
    "@rcp/util.displayname": "^1.0.13",
    "@rcp/util.iscompclass": "^1.0.13"
  },
  "gitHead": "c5b70e409b81139b22686bb20a37b60a5ae1c69a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imcuttle/rcp.git"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
